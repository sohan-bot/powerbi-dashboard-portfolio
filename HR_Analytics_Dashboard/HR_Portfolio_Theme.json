{
  "name": "HR Portfolio Theme - Dark",
  "dataColors": [
    "#2F80ED",
    "#27AE60",
    "#F2C94C",
    "#EB5757",
    "#9B51E0",
    "#56CCF2",
    "#F2994A",
    "#6FCF97"
  ],
  "background": "#1E1E1E",
  "foreground": "#E0E0E0",
  "tableAccent": "#2F80ED",
  "visualStyles": {
    "*": {
      "*": {
        "color": [
          { "solid": { "color": "#E0E0E0" } }
        ],
        "titleText": [
          { "fontSize": 12, "color": { "solid": { "color": "#F2F2F2" } } }
        ],
        "labelText": [
          { "fontSize": 10, "color": { "solid": { "color": "#E0E0E0" } } }
        ]
      }
    },
    "card": {
      "*": {
        "titleText": [
          { "fontSize": 11, "color": { "solid": { "color": "#BDBDBD" } } }
        ],
        "labelText": [
          { "fontSize": 22, "color": { "solid": { "color": "#56CCF2" } } }
        ]
      }
    },
    "slicer": {
      "*": {
        "titleText": [
          { "fontSize": 11, "color": { "solid": { "color": "#E0E0E0" } } }
        ],
        "labelText": [
          { "fontSize": 10, "color": { "solid": { "color": "#E0E0E0" } } }
        ]
      }
    }
  }
}
